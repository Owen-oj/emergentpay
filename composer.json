{
    "name": "owenoj/emergentpay",
    "description": "A Laravel payment package For Emergent payment gateway ",
    "license": "MIT",
    "authors": [
        {
            "name": "Owen Jubilant",
            "email": "owen.j@terktrendz.com",
            "homepage": "https://github.com/owen-oj"
        }
    ],
    "homepage": "https://github.com/owen-oj/emergentpay",
    "keywords": ["Laravel", "EmergentPay","Payment"],
    "require": {
        "illuminate/support": "~5",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Owenoj\\EmergentPay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Owenoj\\EmergentPay\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Owenoj\\EmergentPay\\EmergentPayServiceProvider"
            ],
            "aliases": {
                "EmergentPay": "Owenoj\\EmergentPay\\Facades\\EmergentPay"
            }
        }
    }
}
